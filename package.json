{
  "name": "@eslint/config-inspector",
  "type": "module",
  "version": "0.4.7",
  "packageManager": "pnpm@9.0.5",
  "description": "A visual tool for inspecting and understanding your ESLint flat configs",
  "license": "Apache-2.0",
  "funding": "https://opencollective.com/eslint",
  "homepage": "https://github.com/eslint/config-inspector#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eslint/config-inspector.git"
  },
  "bugs": "https://github.com/eslint/config-inspector/issues",
  "bin": {
    "@eslint/config-inspector": "./bin.mjs",
    "eslint-config-inspector": "./bin.mjs"
  },
  "files": [
    "*.mjs",
    "dist"
  ],
  "scripts": {
    "build": "nuxi build && unbuild",
    "dev": "nuxi dev",
    "prepare": "nuxi prepare && npx simple-git-hooks",
    "start": "node bin.mjs",
    "prepack": "pnpm build",
    "lint": "nuxi prepare && eslint .",
    "typecheck": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": "^8.50.0 || ^9.0.0"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "chokidar": "^3.6.0",
    "esbuild": "^0.20.2",
    "fast-glob": "^3.3.2",
    "find-up": "^7.0.0",
    "get-port-please": "^3.1.2",
    "h3": "^1.11.1",
    "load-tsconfig": "^0.2.5",
    "minimatch": "^9.0.4",
    "mrmime": "^2.0.0",
    "open": "^10.1.0",
    "picocolors": "^1.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.15.0",
    "@eslint/js": "^9.1.1",
    "@iconify-json/carbon": "^1.1.31",
    "@iconify-json/file-icons": "^1.1.8",
    "@iconify-json/logos": "^1.1.42",
    "@iconify-json/ph": "^1.1.12",
    "@iconify-json/simple-icons": "^1.1.100",
    "@iconify-json/svg-spinners": "^1.1.2",
    "@iconify-json/twemoji": "^1.1.15",
    "@iconify-json/vscode-icons": "^1.1.33",
    "@nuxt/eslint": "^0.3.8",
    "@types/connect": "^3.4.38",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/utils": "^7.7.0",
    "@unocss/eslint-config": "^0.59.4",
    "@unocss/nuxt": "^0.59.4",
    "@vueuse/nuxt": "^10.9.0",
    "bundle-require": "^4.0.2",
    "eslint": "^9.1.0",
    "floating-vue": "^5.2.2",
    "fuse.js": "^7.0.0",
    "lint-staged": "^15.2.2",
    "nuxt": "^3.11.2",
    "nuxt-eslint-auto-explicit-import": "^0.0.2",
    "shiki": "^1.3.0",
    "simple-git-hooks": "^2.11.1",
    "textmate-grammar-glob": "^0.0.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vue-tsc": "^2.0.14"
  },
  "pnpm": {
    "patchedDependencies": {
      "bundle-require@4.0.2": "patches/bundle-require@4.0.2.patch"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
