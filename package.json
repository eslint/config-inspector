{
  "name": "@eslint/config-inspector",
  "type": "module",
  "version": "1.4.2",
  "packageManager": "pnpm@10.30.1",
  "description": "A visual tool for inspecting and understanding your ESLint flat configs",
  "license": "Apache-2.0",
  "funding": "https://opencollective.com/eslint",
  "homepage": "https://github.com/eslint/config-inspector#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eslint/config-inspector.git"
  },
  "bugs": "https://github.com/eslint/config-inspector/issues",
  "bin": {
    "@eslint/config-inspector": "./bin.mjs",
    "eslint-config-inspector": "./bin.mjs"
  },
  "files": [
    "*.mjs",
    "dist"
  ],
  "scripts": {
    "build": "nuxt build && tsdown",
    "dev": "nuxt dev",
    "prepare": "nuxt prepare && npx simple-git-hooks",
    "start": "node bin.mjs",
    "prepack": "pnpm build",
    "lint": "nuxt prepare && eslint .",
    "test": "vitest",
    "typecheck": "nuxt typecheck"
  },
  "peerDependencies": {
    "eslint": "^8.50.0 || ^9.0.0"
  },
  "dependencies": {
    "ansis": "catalog:",
    "bundle-require": "catalog:",
    "cac": "catalog:",
    "chokidar": "catalog:",
    "esbuild": "catalog:",
    "h3": "catalog:",
    "tinyglobby": "catalog:",
    "ws": "catalog:"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@eslint/config-array": "catalog:",
    "@iconify-json/carbon": "catalog:",
    "@iconify-json/file-icons": "catalog:",
    "@iconify-json/logos": "catalog:",
    "@iconify-json/ph": "catalog:",
    "@iconify-json/simple-icons": "catalog:",
    "@iconify-json/svg-spinners": "catalog:",
    "@iconify-json/twemoji": "catalog:",
    "@iconify-json/vscode-icons": "catalog:",
    "@nodelib/fs.walk": "catalog:",
    "@nuxt/eslint": "catalog:",
    "@shikijs/langs-precompiled": "catalog:",
    "@shikijs/transformers": "catalog:",
    "@types/connect": "catalog:",
    "@types/ws": "catalog:",
    "@typescript-eslint/utils": "catalog:",
    "@unocss/eslint-config": "catalog:",
    "@unocss/nuxt": "catalog:",
    "@voxpelli/config-array-find-files": "catalog:",
    "@vueuse/nuxt": "catalog:",
    "eslint": "catalog:",
    "find-up": "catalog:",
    "floating-vue": "catalog:",
    "fuse.js": "catalog:",
    "get-port-please": "catalog:",
    "lint-staged": "catalog:",
    "minimatch": "catalog:",
    "mlly": "catalog:",
    "mrmime": "catalog:",
    "nuxt": "catalog:",
    "nuxt-eslint-auto-explicit-import": "catalog:",
    "open": "catalog:",
    "pathe": "catalog:",
    "rolldown": "catalog:",
    "rollup": "catalog:",
    "shiki": "catalog:",
    "simple-git-hooks": "catalog:",
    "textmate-grammar-glob": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "vue-tsc": "catalog:"
  },
  "resolutions": {
    "@nuxt/vite-builder": "https://pkg.pr.new/@nuxt/vite-builder@33682",
    "esbuild": "catalog:",
    "nitropack": "catalog:",
    "rolldown": "catalog:",
    "rollup": "catalog:",
    "vite": "catalog:"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
